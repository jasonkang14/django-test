{
    "executionRoleArn": "arn:aws:iam::730335333809:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
	    "name": "inflearn_docker",
	    "image": "730335333809.dkr.ecr.ap-northeast-2.amazonaws.com/inflearn_docker:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "containerPort": 8000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "django-task",

}
